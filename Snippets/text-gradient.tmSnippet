	<key>content</key>
	<string>@mixin text-gradient(\$start, \$stop) {
	background:-webkit-linear-gradient(top,\$start,\$stop);
	-webkit-text-fill-color: transparent;
	-webkit-background-clip: text;
}</string>
	<key>name</key>
	<string>text-gradient</string>
	<key>scope</key>
	<string>source.css.scss</string>
